@model Entities.DTOs.ContactDetailDto
@{ ViewBag.Title = "Update a Contact Detail";}

<h2>Update a Contact Detail <a asp-action="Index" class="btn btn-sm btn-secondary">Back</a></h2>
<form asp-action="UpdateContactDetail" method="post">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input class="form-control" asp-for="Id" readonly />
    </div>
    <div class="form-group">
        <label asp-for="ContactId"></label>
        <input class="form-control" asp-for="ContactId" readonly />
    </div>
    <div class="form-group">
        <label asp-for="TelephoneNumber"></label>
        <input class="form-control" asp-for="TelephoneNumber" />
    </div>
    <div class="form-group">
        <label asp-for="EMailAddress"></label>
        <input class="form-control" asp-for="EMailAddress" />
    </div>
    <div class="form-group">
        <label asp-for="Address"></label>
        <input class="form-control" asp-for="Address" />
    </div>
    <div class="text-center panel-body">
        <button type="submit" class="btn btn-sm btn-primary">Update</button>
    </div>
</form>